{
    "name": "silverstripe/recipe-core",
    "type": "silverstripe-recipe",
    "description": "SilverStripe framework-only core recipe",
    "homepage": "http://silverstripe.org",
    "license": "BSD-3-Clause",
    "repositories": [
        {
            "type": "vcs",
            "url": "git@github.com:i-lateral/silverstripe-importexport.git"
        }
    ],
    "require": {
        "silverstripe/recipe-plugin": "^1.2",
        "silverstripe/assets": "1.6.1@stable",
        "silverstripe/config": "1.0.18@stable",
        "silverstripe/framework": "4.6.2@stable",
        "silverstripe/mimevalidator": "2.0.0@stable",
        "i-lateral/silverstripe-modeladminplus": "^1.1",
        "i-lateral/silverstripe-gridfield-addons": "^2.0",
        "i-lateral/silverstripe-importexport": "2.x-dev",
        "tractorcow/silverstripe-fluent": "^5.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7",
        "sminnee/phpunit-mock-objects": "^3.4.5",
        "deployer/deployer": "^6.7"
    },
    "extra": {
        "resources-dir": "_resources"
    },
    "config": {
        "process-timeout": 600
    },
    "prefer-stable": true,
    "minimum-stability": "dev"
}
